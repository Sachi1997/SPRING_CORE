<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean name="Emp1" class="com.springcore.collections.Emp">
		<property name="name" value="sachi"/>
		<property name="empNo" value="3434"/>
		<property name="empAddress">
			<set>
				<value>Bbsr</value>
				<value>Bam</value>
				<value>Hyd</value>
			</set>
		</property>
		<property name="empCourses">
			<map>
				<entry key="java" value="3 Months" />
				<entry key=".net" value="2 Months" />
				<entry key="DB" value="2 Months" />
			</map>
		</property>
		<property name="props">
			<props>
				<prop key="name">Sachi</prop>
				<prop key="designation" >Asst. Solution Eng.</prop>
			</props>
		</property>
	</bean>

</beans>